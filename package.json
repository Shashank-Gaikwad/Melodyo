{
  "name": "melodyo",
  "version": "0.1.0",
  "dependencies": {
    "cross-env": "^5.2.1",
    "electron-is-dev": "^1.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "lint": "eslint  **/*.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "Stylish, Powerful and Fast Music Player with elegant design. Melodyo lets you manage all your music files quickly and easily. Browse and play music songs by genres, albums, artists, songs and folder.",
  "main": "src/main.js",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "electron": "^6.0.9",
    "electron-builder": "^21.2.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "wait-on": "^3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shashank-Gaikwad/melodyo.git"
  },
  "keywords": [
    "music",
    "player",
    "desktop"
  ],
  "author": "Shashank Gaikwad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shashank-Gaikwad/melodyo/issues"
  },
  "homepage": "https://github.com/Shashank-Gaikwad/melodyo#readme"
}
